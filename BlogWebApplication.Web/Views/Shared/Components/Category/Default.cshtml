@model IEnumerable<Category>

<ul class="list-group list-group-flush">
    <li class="list-group-item"><h5>Kategoriler</h5> </li>
    @foreach(var item in Model)
    {
        <a asp-action="Index" asp-controller="Blog" class="list-group-item list-group-item-action"  asp-route-catId="@item.CategoryID" >@item.CategoryName</a>
    }
</ul>

